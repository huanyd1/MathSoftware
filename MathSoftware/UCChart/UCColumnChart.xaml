<UserControl x:Class="MathSoftware.UCChart.UCColumnChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MathSoftware.UCChart" 
             xmlns:Wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d"
             Loaded="UCColumnChart_Loaded"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Wpf:CartesianChart x:Name="ColumnChart">
            <Wpf:CartesianChart.AxisX>
                <Wpf:Axis x:Name="axisX" Position="LeftBottom"></Wpf:Axis>
            </Wpf:CartesianChart.AxisX>
            <Wpf:CartesianChart.AxisY>
                <Wpf:Axis x:Name="axisY"></Wpf:Axis>
            </Wpf:CartesianChart.AxisY>
        </Wpf:CartesianChart>
    </Grid>
</UserControl>
